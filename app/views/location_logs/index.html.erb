

<h1>Listing Location Logs</h1>

<table>
  <thead>
    <tr>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Position time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_logs.each do |location_log| %>
      <tr>
        <td><%= location_log.latitude %></td>
        <td><%= location_log.longitude %></td>
        <td><%= location_log.position_time %></td>
        <td><%= link_to 'Show', location_log %></td>
        <td><%= link_to 'Edit', edit_location_log_path(location_log) %></td>
        <td><%= link_to 'Destroy', location_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location log', new_location_log_path %>

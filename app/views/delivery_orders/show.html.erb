<table class="table">
  <tr>
    <td colspan=2><strong>Matchid:</strong> <%= @delivery_order.matchID_id %></td>
  </tr>
   <tr>
    <td><strong>Driver agreed:</strong> <%= @delivery_order.isSigned_Driver %></td>
    <td><strong>Receiver agreed:</strong> <%= @delivery_order.isSigned_Receiver %></td>
  </tr>
  <tr>
    <td colspan=2> <strong>Destiny date:</strong> <%= (@delivery_order.destiny_date).strftime("%d/%m/%Y %I:%M%p") %></td>
  </tr>
  <tr>
    <td colspan=2> <strong>Comment:</strong> <%= @delivery_order.comment %></td>
  </tr>
</table>


<%= link_to 'Edit', edit_delivery_order_path(@delivery_order) %> |
